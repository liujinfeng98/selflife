<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
 PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.selflife.dao.UserDao">
            
         <insert id="insertUser" parameterType="User" keyProperty="ec_userid">
             insert into bd_user_dt( 
         ec_username,
         ec_password,
         	ec_userlevel)
         values
         (       
         #{ec_username},
         #{ec_password},
         	#{ec_userlevel}
         	)
         </insert>
          
</mapper>
